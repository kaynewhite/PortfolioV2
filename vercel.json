{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "script.js",
      "use": "@vercel/static"
    },
    {
      "src": "client/src/assets/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/contact",
      "dest": "/api/contact.js"
    },
    {
      "src": "/api/cv/download",
      "dest": "/api/cv-download.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/client/src/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/contact.js": {
      "runtime": "nodejs18.x"
    },
    "api/cv-download.js": {
      "runtime": "nodejs18.x"
    }
  }
}